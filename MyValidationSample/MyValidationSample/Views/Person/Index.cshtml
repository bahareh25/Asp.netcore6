@model List<Person>

    <table>
        <tr>
            <td>Id</td>
            <td>FirstName</td>
            <td>LastName</td>
        </tr>
   @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
        </tr>
    }
</table>
