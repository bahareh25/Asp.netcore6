@using  MyViewComponentSample.Models
@model List<News>
<table style="border-color:red; border-style:dotted">
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>Description</th>

        </tr>
        @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>
            <td>@item.Description</td>
        </tr>

    }
    </table>